<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appenders>
        <console name="stdout" target="SYSTEM_OUT">
            <patternLayout pattern="[%-5level] %d{yyyy-MM-dd HH:mm:ss.SSS} [%t] %c{1}: %msg%n" />
        </console>
    </appenders>

    <loggers>
        <root level="info">
            <appenderRef ref="stdout" />
        </root>
    </loggers>
</configuration>